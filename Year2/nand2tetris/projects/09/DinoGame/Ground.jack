class Ground {
    
    constructor Ground new() {
        do draw();
        return this;
    }

    method void draw() {
        do Screen.setColor(true);
        do Screen.drawRectangle(1, 232, 254, 235);
        return;
    }
    
    method void dispose() {
      do Memory.deAlloc(this);
      return;
   }
}